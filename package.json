{
  "name": "clusterws",
  "version": "4.0.0-alpha.0",
  "description": "ClusterWS - lightweight, fast and powerful framework for building scalable WebSocket applications in Node.js.",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Dmitrii Goriunov",
  "scripts": {
    "test": "nyc mocha \"./tests/**/*.ts\" --exit",
    "build": "ts-builder -c ./tsbuild.json",
    "release": "npm run build && cd dist && npm publish --tag alpha --access public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClusterWS/ClusterWS"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-builder": "^0.9.2",
    "ts-node": "^7.0.1",
    "uws": "^10.148.1",
    "ws": "^6.2.0"
  },
  "keywords": [
    "clusterws",
    "websocket",
    "scale",
    "cws",
    "node js websocket",
    "scale node js websocket",
    "real-time",
    "cluster",
    "javascript"
  ],
  "dependencies": {
    "@clusterws/cws": "^0.12.0"
  },
  "nyc": {
    "temp-directory": "./node_modules/.cache/nyc_output",
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "json",
      "html"
    ]
  }
}
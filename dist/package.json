{
	"name": "clusterws",
	"version": "4.0.0-alpha.0",
	"description": "ClusterWS - lightweight, fast and powerful framework for building scalable WebSocket applications in Node.js.",
	"main": "index.js",
	"types": "index.d.ts",
	"author": "Dmitrii Goriunov",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ClusterWS/ClusterWS"
	},
	"keywords": [
		"clusterws",
		"websocket",
		"scale",
		"cws",
		"node js websocket",
		"scale node js websocket",
		"real-time",
		"cluster",
		"javascript"
	],
	"dependencies": {
		"@clusterws/cws": "^0.12.0"
	},
	"nyc": {
		"temp-directory": "./node_modules/.cache/nyc_output",
		"include": [
			"src/**/*.ts"
		],
		"extension": [
			".ts"
		],
		"require": [
			"ts-node/register"
		],
		"reporter": [
			"text",
			"json",
			"html"
		]
	}
}